@model IEnumerable<Book>

@{
    ViewData["Title"] = "Remained Books";
}




<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Author</th>
            <th scope="col">BookCayrgory</th>

            <th scope="col">Book Count</th>
            
        </tr>
    </thead>
    @{
        var i = 1;
    }
    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <th scope="row">@i</th>
                <td>@item.BookId</td>
                <td>@item.Title</td>
                <td>@item.Author.FullName</td>
                <td>@item.BookCategory</td>
                <td>@item.BookCount</td>

            </tr>
        </tbody>
        i++;
    }

</table>